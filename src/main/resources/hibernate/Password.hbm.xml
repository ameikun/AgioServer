<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="all">
 <class name="com.cqu.agio.entity.Password" table="password">
  <id column="userID" name="userID" type="string">
   <generator class="assigned"/>
  </id>
  <property  generated="never" lazy="false" name="nickname" not-null="true" type="string"/>
  <property  generated="never" lazy="false" name="phone" type="string"/>
  <property  generated="never" lazy="false" name="email" type="string"/>
  <property  generated="never" lazy="false" name="password" not-null="true" type="string"/>
  <property  generated="never" lazy="false" name="updateDate" type="timestamp"/>
 </class>
</hibernate-mapping>